{
  "name": "uddaan-consultancy",
  "version": "1.0.0",
  "description": "Uddaan Consultancy - Your Gateway to Global Opportunities",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm start",
    "start": "cd backend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "seed": "cd backend && node seedData.js",
    "setup": "./start.sh",
    "health": "curl -f http://localhost:5000/api/health || echo 'Backend not running'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "education",
    "consulting",
    "jobs",
    "international"
  ],
  "author": "Uddaan Consultancy",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^6.13.0"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "nodemailer": "^7.0.5",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.3",
    "speakeasy": "^2.0.0"
  }
}
